# ip[:port]
DNSCRYPT_LOCAL_ADDRESS='127.0.0.1'

# default is /var/log/${SVCNAME}.log, one can set it to /path/to/file or special 'syslog' value
DNSCRYPT_LOGGER=

# default is dnscrypt
DNSCRYPT_USER=

# default is dnscrypt (not sure *really* needed)
DNSCRYPT_GROUP=

# 'name' or 'ip'
DNSCRYPT_MODE='name'

# --loglevel=, --max-active-requests=, --ephemeral-keys etc
DNSCRYPT_OPTS=

### 'name' variables:

# default is ${SVCNAME#dnscrypt-proxy.}"
DNSCRYPT_RESOLVER_NAME=
# default is /usr/share/dnscrypt-proxy/dnscrypt-resolvers.csv
DNSCRYPT_RESOLVERS_FILE=

### 'ip' variables:

# ip:port, mandatory
DNSCRYPT_PROVIDER_ADDRESS=
# mandatory
DNSCRYPT_PROVIDER_NAME=
# mandatory
DNSCRYPT_PROVIDER_KEY=
